<Window x:Class="Final_Practice.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Final_Practice"
        mc:Ignorable="d"
        Title="AdminWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <ComboBox Name="PageSelection" Margin="8" HorizontalAlignment="Left" VerticalAlignment="Center" SelectionChanged="onSelectedPageChanged" />
        <Frame Name="AdminPages" Grid.Row="1" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
